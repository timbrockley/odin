package main

import "core:fmt"
import lm "libmath"

main :: proc() {
	//----------------------------------------
	a: i32 = 5
	b: i32 = 3
	result: i32
	//----------------------------------------
	result = lm.add(a, b)
	//----------------------------------------
	fmt.printfln("add(%d, %d) = %d", a, b, result)
	//----------------------------------------
	result = lm.sub(a, b)
	//----------------------------------------
	fmt.printfln("sub(%d, %d) = %d", a, b, result)
	//----------------------------------------
}
